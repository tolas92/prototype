[Unit]
Description=Mount unit for snapd-desktop-integration, revision 15
After=snapd.mounts-pre.target
Before=snapd.mounts.target
Before=local-fs.target

[Mount]
What=/var/lib/snapd/snaps/snapd-desktop-integration_15.snap
Where=/snap/snapd-desktop-integration/15
Type=squashfs
Options=nodev,ro,x-gdu.hide,x-gvfs-hide
LazyUnmount=yes

[Install]
WantedBy=snapd.mounts.target
WantedBy=multi-user.target

[Unit]
Description=Mount unit for ubuntu-mate-welcome, revision 721
After=snapd.mounts-pre.target
Before=snapd.mounts.target
Before=local-fs.target

[Mount]
What=/var/lib/snapd/snaps/ubuntu-mate-welcome_721.snap
Where=/snap/ubuntu-mate-welcome/721
Type=squashfs
Options=nodev,ro,x-gdu.hide,x-gvfs-hide
